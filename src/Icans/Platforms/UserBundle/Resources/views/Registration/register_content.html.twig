<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="user-registration-form">
    {{ form_widget(form) }}
    <div class="user-registration-actions">
        <input type="submit" value="{{ 'registration.submit'|trans }}" class="user-registration-primary-action" id="signup-submit" />
    </div>
</form>
